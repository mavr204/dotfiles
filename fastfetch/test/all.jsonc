{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small",
        "padding": {
            "top": 2
        }
    },
    "modules": [
        "title",
        "separator",
        "os",
        "host",
        "bios", //long
        "bootmgr", //long*
        "kernel",
        "uptime",
        "loadavg", //long*
        "processes", //long*
        "packages", //long*
        "shell", 
        "editor", //long*
        "display", //long*
        "brightness",
        "monitor",//long*
        "lm",
        "de",
        "wm",
        "theme",
        "icons",//long*
        "font",//long*
        "cursor",//long*
        "terminal",
        "terminalfont",//long*
        {
            "type": "cpu",
            "showPeCoreCount": true,
            "temp": true
        },
        "cpuusage",
        {
            "type": "gpu",
            "driverSpecific": true,
            "temp": true
        },
        "memory",
        "physicalmemory", //long
        // long {
        //     "type": "swap",
        //     "separate": true
        // },
        "disk", //long
        //"btrfs",
        //"zpool",
        {
            "type": "battery",
            "temp": true
        },
        {
            "type": "publicip",
            "timeout": 1000
        },
        {
            "type": "localip",
            "showIpv6": true,
            "showMac": true,
            "showSpeed": true,
            "showMtu": true,
            "showLoop": true,
            "showFlags": true,
            "showAllIps": true
        },
        "dns",//long*
        "wifi",
        "locale",//long
        "vulkan",//long
        "opengl",//long
        "opencl",//long
        "sound",//long
        "camera",//long
        {
          "type": "weather",
          "timeout": 1000
        },
        "netio",
        "diskio",
        // long{
        //     "type": "physicaldisk",
        //     "temp": true
        // },
        "tpm",
        "break",
        "colors"
    ]
}
